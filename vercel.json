{
    "version": 2,
    "builds": [
      {
        "src": "project/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "buildCommand": "./build_files.sh"
        }
      },
      {
        "src": "staticfiles_build/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles_build/$1"
      },
      {
        "src": "/(.*)",
        "dest": "project/wsgi.py"
      }
    ]
  }
  